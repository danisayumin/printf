#include	"ft_printf.h"

int	ft_putunsign(unsigned int nbr)
{
	int	count;

	count = 0;
	if (nbr > 9)
		count += ft_putnbr(nbr / 10);
	write(1, &"0123456789"[nbr % 10], 1);
	return (count);
}
