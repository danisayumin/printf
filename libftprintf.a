#include	"ft_printf.h"

int	ft_putstr(char *s)
{
	int	i;
	int	count;

	if (!s)
		return (ft_putstr("(null)"));
	i = 0;
	count = 0;
	while (i < ft_strlen(s))
	{
		write(1, &s[i], 1);
		i++;
		count++;
	}
	return (count);
}
