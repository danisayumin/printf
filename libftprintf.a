#include "ft_printf.h"

char	ft_putptr(unsigned long nbr, int i)
{
	int	count;

	count = 0;
	if (nbr == 0)
	{
		ft_putstr("(nil)");
		return (5);
	}
	if (i == 0)
	{
		ft_putstr("0x");
		count += 2;
	}
	if (nbr > 15)
		count += ft_putptr(nbr / 16, ++i);
	write(1, &"0123456789abcdef"[nbr % 16], 1);
	return (count);
}

