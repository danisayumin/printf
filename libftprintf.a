#include	"ft_printf.h"

int	ft_puthex_low(unsigned long n)
{
	int	count;

	count = 0;
	if (n >= 16)
		count += ft_puthex_low(n / 16);
	write(1, &"0123456789abcdef"[n % 16], 1);
	return (count);
}
